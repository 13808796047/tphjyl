    <script>
        function getRandom(min, max) {
            return Math.floor(Math.random() * (max - min)) + min;
        }
        function checkBank() {
            var v = $("#bank_div :radio").is(":checked");
            if (v === false) {
                $("#bank-msg").html("请先选择银行");
            }
        }

        $("#alipay-money").on('keyup', function(r) {

            $(this).val(formatFloat($(this).val()));

            var oldMoney = new Number($("#alipay-money").val());

            if (isNaN(oldMoney)) {
                oldMoney =0;
            }

            var newMoney = 0;

            if (oldMoney==0) {
                newMoney = 0;
            }
            else if (oldMoney < 21) {
                var ra = (getRandom(1, 50) / 100);
                newMoney = (oldMoney) + ra;
            }
            else if (oldMoney >= 21 && oldMoney < 100) {
                newMoney = oldMoney + (getRandom(1, 100) / 100);
            }
            else {
                newMoney = oldMoney - (getRandom(1, 100) / 100);
            }
            newMoney = Math.floor(newMoney*100)/100;
            $("#alipay-money-ture").html(newMoney);
        });

        $(function () {
            $('#wechat-money, #bank-money, #alipay-bank-money, #baokim-money, #bankatm-amount, #atm-money, #unionpay_nocard-money, #qqpay-money').keyup(function() {
                $(this).val(formatFloat($(this).val()));
            });
            $("#owl-slider").owlCarousel({
                navigation: true,
                slideSpeed: 300,
                singleItem: true,
                lazyLoad: true,
                navigationText: ['<div class="arrow"><div class="arrow_left"></div><div>', '<div class="arrow"><div class="arrow_right"></div></div>']
            });


            //隱藏全部银行
            $('div.set.bank .new-cards > span').addClass('hidden');

