            //            $('.set.nope').removeClass('hidden');
            //            $('.set.nope #title').html("您没有操作权限");
            //        }

            /**
             * 銀行付款
             */
            $("#send-bank").on('click', function(r) {
                var maintain_flag = 0;
                var not_maintain = true;
                if ($("#bank-money").val()) {
                    if ($("#bank-money").val() < Number($("#bank-min").html())) {
                        $("#bank-msg").html("金额低于最低限制")
                        $('#bank-notice').html('');
                        return false;

                    } else if ($("#bank-money").val() > Number($("#bank-max").html())) {
                        $("#bank-msg").html("金额高于最高限制")
                        $('#bank-notice').html('');
                        return false;

                    } else {

                    }
                    if (maintain_flag) {
                        var temp = new Array();
                        temp = $("input[name='bk']:checked").val();
                        var bank_maintain = null;

                        $.each(bank_maintain, function(i, val) {
                            var alert_txt = val.begindate + ' - ' + val.enddate + ' ' + val.title + '，请选择其他银行发起充值',
                                    span_txt = '<font class="maintain_date"> ' + val.begindate + ' - ' + val.enddate + '</font> ' + val.title + '，请选择其他银行发起充值';

                            if (temp == i) {
                                $("#bank-msg").html('');
                                $('#bank-notice').html(span_txt);
                                not_maintain = false;
                                return false;
                            }
                        }); //end $.each
                    } //end if(maintain_flag)
                    if (not_maintain) {
                    }
                } else {
                    $("#bank-msg").html("请输入金额")
                    $('#bank-notice').html('');
                }
            });

            $("#send-wechat").on('click', function(r) {
                if ($("#wechat-money").val()) {
                    if ($("#wechat-money").val() < Number($("#wechat-min").html())) {
                        $("#wechat-msg").html("金额低于最低限制")
                    } else if ($("#wechat-money").val() > Number($("#wechat-max").html())) {
