            $("#send-alipay-bank").on('click', function(r) {
                if ($("#alipay-bank-money").val() == '') {
                    $("#alipay-bank-msg").html("请输入金额")
                } else if (isNaN($("#alipay-bank-money").val())) {
                    $("#alipay-bank-msg").html("金额输入错误")
                } else if ($("#alipay-bank-money").val()) {
                    if ($("#alipay-bank-money").val() < Number($("#alipay-bank-min").html())) {
                        $("#alipay-bank-msg").html("金额低于最低限制")
                    } else if ($("#alipay-bank-money").val() > Number($("#alipay-bank-max").html())) {
