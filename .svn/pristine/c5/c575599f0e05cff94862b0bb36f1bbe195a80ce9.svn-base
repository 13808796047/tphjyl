	function onekey_keeppoint(){
//一键自动设置
		$("#onekey").val("onekey");
		var keeppoint=$('#keeppoint').val();
		var keeppoint_min=floatval($("#keeppoint_min").html());
		var keeppoint_max=floatval($("#keeppoint_max").html());
		if(keeppoint<keeppoint_min){
			keeppoint=keeppoint_min;

		}else if(keeppoint>keeppoint_max){
			keeppoint=keeppoint_max;
		}
		$('#keeppoint').val(keeppoint);
		if(!confirm("您确定将【自身保留返点】设置为【"+keeppoint+"%】？")){
			return(false);
		}
		var form = jQuery('#lotteriesform');
		var boxes = jQuery(":checkbox[id!='selectalllottery']",form);
		var total = boxes.length;
		for(var i=0;i<total;i++){
			if(!jQuery(boxes[i]).attr('checked')){
				jQuery(boxes[i]).attr("checked",true);
			}
			var id = jQuery(boxes[i]).attr('id').replace('lottery_','');
			var maxp = Number(jQuery("#max_point_"+id).val());//最大允许
			var minp = Number(jQuery("#min_point_"+id).val());//最小一般是0
			var maxip = Number(jQuery("#max_indefinite_point_"+id).val());//不定位最大
			var minip = Number(jQuery("#min_indefinite_point_"+id).val());//不定位最小一般是0
			var point=maxp+fandianjiange-keeppoint;
			var ipoint=maxip+fandianjiange-keeppoint;
			if(point < 0 ){point=0;}
			if(ipoint < 0 ){ipoint=0;}
			jQuery("#point_"+id).val(floatval(point));
			jQuery("#indefinite_point_"+id).val(floatval(ipoint));
		}

	}
	function floatval(num){
		return(parseFloat(parseFloat(num).toFixed(14)));
	}
	var fandianjiange=0;
	function onekey_init(){
		var form = jQuery('#lotteriesform');
		var boxes = jQuery(":checkbox[id!='selectalllottery']",form);
		var total = boxes.length;
		var _min=0;
		var _max=0;
		for(var i=0;i<total;i++){
			var id = jQuery(boxes[i]).attr('id').replace('lottery_','');
			var minp = Number(jQuery("#min_point_"+id).val());
			var minip = Number(jQuery("#min_indefinite_point_"+id).val());
			var min=(minp>minip)?minip:minp;
			var maxp = Number(jQuery("#max_point_"+id).val());
			var maxip = Number(jQuery("#max_indefinite_point_"+id).val());
			var max=(maxp<maxip)?maxip:maxp;//

			_min=(min>_min)?min:_min;
			_max=(max>_max)?max:_max;
		}
		//$("#keeppointlevel").html(_max+fandianjiange);
		$("#keeppoint_min").html(floatval(fandianjiange));
		$("#keeppoint_max").html(floatval(_max+fandianjiange));
	}
	function selectAllLottery(obj)
	{

		var objs = jQuery(jQuery("input[type='text']"),jQuery('#lotteriesform'));
		objs.each(function(){
			if(jQuery(this).val() == '')
			{
				jQuery(this).val('0');
			}
		});
		jQuery("input[id^='lottery_']").prop('checked', obj.checked);
	}
	function filterPercent(num){
		num = num.replace(/^[^\d]/g,'');
		num = num.replace(/[^\d.]/g,'');
		num = num.replace(/\.{2,}/g,'.');
		num = num.replace(".","$#$").replace(/\./g,"").replace("$#$",".");
		if( num.indexOf(".") != -1 ){
			var data = num.split('.');
			num = (data[0].substr(0,3))+'.'+(data[1].substr(0,1));
		}else{
			num = num.substr(0,3);
		}
		return num;
		num = num > 100 ? 100 : num;
		return num;
	}
