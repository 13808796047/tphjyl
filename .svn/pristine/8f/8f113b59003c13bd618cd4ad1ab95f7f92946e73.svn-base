/* table */
.table{width:100%;}
.table th,.table td{padding:6px 12px;border-bottom:1px solid #E3E3E3;text-align:left;}
.table-border th,.table-border td{border:1px solid #E3E3E3;}
.table-border th{background-color:#FAFAFA;}
.table-border td{background-color:#FFF;}
.table-info thead{
              box-shadow:0 2px 0 rgba(0, 0, 0, 0.05);
    -moz-box-shadow:0 2px 0 rgba(0, 0, 0, 0.05);
-webkit-box-shadow:0 2px 0 rgba(0, 0, 0, 0.05);
}
.table-center{
	text-align: center;
}
.table-center td,
.table-center th{
	text-align: center!important;
}
.table-info th,.table-info td{text-align:center;}
.table-info th{padding-top:10px;padding-bottom:10px;}
.table-info td{border-right:1px solid #E3E3E3;border-bottom-style:dotted;padding-top:8px;padding-bottom:8px;}
.table-info .highlight-cell td{ color:#f00;}
.table-border-none th,.table-border-none td{border:0 none;}
.table-info .text-left{text-align:left;}
.table-info .text-right{text-align:right;}
.table-info .text-center{text-align:center;}

.table{
	border-collapse:collapse;
	border-right:1px solid #E3E3E3;
}
.table .thumbnail{width:40px;height:40px;}
.table-bg{margin:10px 20px 0;padding:0 20px;border:1px dashed #DEDEDE;}
.table-bg tr:last-child td{border-bottom:0 none;}
.table-bg1{background-color:#EDFCE8;}
.table-bg2{background-color:#FDFDDF;}
.table-bg3{background-color:#FDE9E9;}
.table-bg4{background-color:#FCF2E4;}
.table-bg5{background-color:#F2F2F2;color:#999;}
.table-info{border-top:1px solid #DEDEDE;}
.table-info th,.table-info td{text-align:left;white-space:nowrap;}
.table-info th{
background:-webkit-linear-gradient(top,#FFF,#F7F7F7) #F3F3F3;
background:    -moz-linear-gradient(top,#FFF,#F7F7F7) #F3F3F3;
background:         -o-linear-gradient(top,#FFF,#F7F7F7) #F3F3F3;
background:              linear-gradient(top,#FFF,#F7F7F7) #F3F3F3;
border-color:#DEDEDE;
border: 1px solid #E3E3E3;
}


.table-function th{font-weight:700;}
.table-function td{border-right:1px solid #E3E3E3;border-bottom:1px solid #E3E3E3;}
.table-function thead{box-shadow:none;}
.table-function .sp-td{background: url("table-data-sj.png") no-repeat scroll right bottom transparent;cursor:pointer;
              box-shadow:0 2px 0 rgba(0, 0, 0, 0.05);
    -moz-box-shadow:0 2px 0 rgba(0, 0, 0, 0.05);
-webkit-box-shadow:0 2px 0 rgba(0, 0, 0, 0.05);
}
.table-function .sp-td-focus{background:#FCF9F1;}
.table-function .sp-td-selected{background: url("table-data-selected.png") no-repeat scroll right top #FCF9F1;}
.table-function .sp-td-cont{position:relative;}
.table-function .sp-td-havevalue {background:#FCF9F1;}
.sp-filter-close {position:absolute;right:-6px;top:-25px;width:18px;height:18px;background:url("table-data-selected.png") no-repeat;display:none;}
.table-function .sp-td-havevalue .sp-filter-close {display:block;}
.table-function .sp-td-havevalue .sp-td-cont-b .sp-filter-close {top:-5px;}

.table td:hover{
	background: #dcedeb!important;
}


.choose-multiple,
.choose-model{
	position:relative;
	display:inline-block;
	*display:inline;
	*zoom:1;
	background-color: #fff;
}
.choose-multiple .input{position:relative;width:46px;padding:3px;font-weight:700;text-align:center;color:#097969;}
.choose-model .info{
	display:inline-block;
	width: 79px;
	height: 19px;
	line-height:18px;
	padding:3px;
	color: #ffdd81;
	font-weight:700;
	text-align:center;
	vertical-align:middle;
	font-size:12px;
}
#pppsssi .info{
    width: 162px;
}


.choose-model .choose-input {
	width: 100%;
	border: 0;
	text-align: center;
	background: none;
	color: #000;
	outline: none;
}
.choose-multiple i,
.choose-model i{
	display:inline-block;
	width:25px;
	height:25px;
	margin-left:-1px;
	vertical-align:middle;
	background: url("select-down.png") -2px -2px #e7e7e7 no-repeat;
	cursor:pointer;
}
.choose-multiple .text,.choose-model .text{display:inline-block;padding-left:6px;vertical-align:middle;}

.chase-trace-startNumber-select .choose-list {height:260px;overflow-y:auto;}

.bet-statistics .total-money,
.bet-statistics .choose-bet{
	text-align: left;
}

.bet-statistics .choose-bet{
	margin-top: 15px;
}

.choose-model .slide-checkbox{
	position: relative;
	display: inline-block;
	*zoom: 1;
	*display: inline;
	top: 0;
	left: 0;
	width: 53px;
	height: 28px;
	background: none repeat scroll 0 0 #79A5B7;
	box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3) inset;
	border-radius: 3px;
	vertical-align: middle;
}

.choose-model .slide-checkbox .checklist{
	position: absolute;
	left:2px;
	top:2px;
	margin: 0;
	padding: 0;
	height: 24px;
	z-index: 2;
}

.choose-model .slide-checkbox .checklist a{
	display: inline-block;
	height: 24px;
	width: 24px;
	color: #FFF;
	text-decoration: none;
	vertical-align: middle;
	-moz-transition: color 0s ease 0.2s;
	-webkit-transition: color 0s ease 0.2s;
	-o-transition: color 0s ease 0.2s;
	transition: color 0s ease 0.2s;
}

.choose-model .slide-checkbox .checklist a.current{
	color: #666;
	font-weight: 700;
}

.choose-model .slide-checkbox .current-check-background{
	position: absolute;
	top: 2px;
	left: 2px;
	width: 24px;
	height: 22px;
	border-bottom:2px solid #DEDEDE;
	background-color:#FFF;
	border-radius: 2px;
	-moz-transition: left 0.2s ease;
	-webkit-transition: left 0.2s ease;
	-o-transition: left 0.2s ease;
	transition: left 0.2s ease;
}

.choose-list{
	position:absolute;
	top: 25px;
	left:0;
	padding:5px 0;
	font-size: 12px;
	border:1px solid #28847C;
	z-index:1;
	overflow: auto;
	max-height: 350px;
}
.choose-list a{
	display: block;
	margin: 0px 0;
	padding: 0 14px;
	height: 25px;
	line-height: 25px;
	text-align: left;
	color: #28847C;
	border-bottom: 1px solid #ccc;
}
.choose-list a:hover{background-color:#28847C;color:#FFF;text-decoration:none;}
.choose-multiple .choose-list{width:78px;}
.choose-model .choose-list{
	width: 108px;
	background-color: #fff;
}

#pppsssi  .choose-list{
	width: 188px;
	background-color: #fff;
}

