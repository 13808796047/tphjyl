#lan_en .self-rebate + input, #lan_vm .self-rebate + input {margin-left: 30px !important;}
/**/

#lan_en .not-table, #lan_vm .not-table {font-size: 13px;}

#lan_en .form .field_fix,
#lan_vm .form .field_fix {margin: 0 0 0 -120px;}

#lan_en .form .field_fix label,
#lan_vm .form .field_fix label {
  position: static;
  margin: 0px;
  width: auto;
}
#lan_en .card-after{
  position: relative;
  width: 136px;
  display: inline-block;
  vertical-align: middle;
}
#lan_vm .card-after{
  position: relative;
  width: 160px;
  display: inline-block;
  vertical-align: middle;
}

/*充值:按鈕箭頭*/
.arrow{
    height:10px;
}
.arrow_left {
  border-width:4px;
  border-style:solid;
  border-color:transparent #000 transparent transparent;
  width: 0;
  height: 0;
  margin: 3px 0px 3px 0px;
}
.arrow_right {
  border-width:4px;
  border-style:solid;
  border-color:transparent transparent transparent #000;
  width: 0;
  height: 0;
  margin: 3px 0px 3px 0px;
}

@media screen and (max-width:1080px) {
  html {
    -webkit-text-size-adjust: 100%;
       -moz-text-size-adjust: 100%;
      -ms-text-size-adjust: 100%;
        text-size-adjust: 100%;
  }
}/*end media 1080px*/
/*20170623-bobo新增帐变页面下级浮动方块*/
.userchild_name{
  position: relative;
}
.userchild_name span{
  position: absolute;  
  background: #f2f2f2;
  color: #000;
  z-index: 1;
  font-size: 12px;
  min-width: 60px;
  padding: 0.2em;
  left: 30%;
  top: 80%;
  display: none;
}
.userchild_name:hover span{
  display: block;
}