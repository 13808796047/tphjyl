
body {
  margin: 0;
  padding: 0;
  font: 12px 'Microsoft Yahei', 'Microsoft JhengHei', tahoma, Srial, helvetica, sans-serif;
}
.ys-content {
  background: #e0e0e0;
  color: #595959;
  padding: 0 0 10px;
}
.ys-content a {
  color: #262626;
}
.ys-content .header {
  color: #045851;
  font-size: 18px;
  font-weight: 700;
  height: 20px;
  padding: 15px 30px;
  width: 715px;
}
.ys-content .main{
  border: none;
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;
  min-height: 621px;
}
.ys-content .green1 {color: #ff3300;}
.section {
  padding: 10px;
  font-size: 14px;
  position: relative;
  width: 755px;
}
.pd6 {
  padding-bottom: 6px !important;
}
.h105 {
  border: 1px solid #cecece;
  height: 105px;
  margin: 0 auto 10px;
  width: 733px;
}
.section.h105:before {
  content: '';
  width: 0;
  height: 100%;
  display: inline-block;
  position: relative;
  vertical-align: middle;
}
.form {
  margin-bottom: 0;
}
.not-table {
  border-collapse: collapse;
  font-size: 14px;
}
.not-table td {
  display: inline-block;
  padding: 10px 0;
}

.form .field , tr.tr-field {
  padding: 10px 0;
  width: 755px;
}

.form .field, .tr-field {border-bottom: 1px solid #d9d9d9;}
.form .field label {
  display: inline-block;
  margin-right: 3px;
  min-width: 119px;
  text-align: left;
}
.form .field .user_type_radio, .form .field .user_type_checkbox {
  margin: 0;
  min-width: 0;
  cursor: pointer;
}
.form .inline-fields {border-bottom: 1px solid #d9d9d9;}
.form .inline-fields .field {
  border-bottom: none;
  display: inline-block;
  display: inline !important\9;
  width: auto;
}
.form .inline-fields .field + .field label {min-width: 0;margin: 0 0 0 6px;}
form input[type="text"], form input[type="password"] {
  background: #fff;
  border: 1px solid #cecece;
  color: #595959;
  font: 14px 'Microsoft Yahei', 'Microsoft JhengHei', tahoma, Srial, helvetica, sans-serif;
  width: 141px;
  height: 28px;
  border-radius: 2px;
  padding: 0 6px;
}
.section .selection {
  width: 155px;
}
.btn, .alert_btn, form input[type="submit"], form input[type="button"] {
  background: #0c7b70;
  border: none;
  min-width: 95px;
  height: 30px;
  color: #fff;
  border-radius: 2px;
  cursor: pointer;
  text-shadow: 0 1px 1px #444;
  margin: 0 17px 17px 17px;
  font-family: Microsoft YaHei;
  position: relative;
  top: 2px;
  display: inline-block;
  text-align:center;
  text-decoration:none;
  line-height: 26px;
}
.alert_btn {
  background: #ee8802;
}
.form .btn, form input[type="submit"] {
  margin: 20px 17px 0 126px;
}

.btn:hover, form input[type="button"]:hover, form input[type="submit"]:hover {
  background: #018f81;
}
.alert_btn:hover {
  background: #ff8802;
}
a.btn {color: #fff;}
.cur .btn {
  background: #454545;
}
.section .currentpoint {
  color: #ff3802;
}
.btn.margin0 {margin: 0;}
.form .text-input {
  width: 142px;
  height: 28px;
  border-radius: 2px;
  border: none;
  padding: 0 8px;
}
.section .select2-container {
  height: 30px;
}
.section .select2-container .select2-selection--single {
  height:30px;
  border-radius: 2px;
}
.section #select2-keeppoint-container {
  padding-left: 12px;
  padding-right: 30px;
}
.section .select2-selection__arrow {
  width: 30px;
  height: 28px;
  border-left: 1px solid #ccc;
}
.section .select2-selection__arrow b {
  border-width: 6px 5px 0 4px;
  border-color: #656974 transparent transparent transparent;
}
.section .text {
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
  height: 24px;
  font-size: 16px;
}
.section .url-divider {
  position: absolute;
  left: 165px;
  top: 0;
  bottom: 0;
  margin: auto;
  height: 73px;
  width: 14px;
}
.section .urls {
  display: inline-block;
  position: relative;
  vertical-align: middle;
  margin: 0;
  list-style-type: none;
  padding: 0 5px;
  font-size: 12px;
  left: 20px;
}
.section .urls li {
  height: 36px;
  line-height: 36px;
  position: relative;
}
.section .urls li:before {
  background: #cddcdb;
  content: "";
  height: 1px;
  position: absolute;
  left: -19px;
  top: 17.5px;
  width: 13px;
}
.section .urls li + li:after {
  background: #cddcdb;
  content: "";
  height: 36px;
  position: absolute;
  left: -19px;
  top: -18px;
  width: 1px;
}
.section .urls li a {
  margin-right: 10px;
}
.section .urls button {
  border: none;
  color: #fff;
  background: #007c70;
  min-width: 95px;
  height: 30px;
  font-size: 12px;
  border-radius: 2px;
  cursor: pointer;
  padding: 0 5px;
}
.section .urls button .copy {
  background: url(../images/matt/copy.png);
  display: inline;
  padding: 0 7px;
  background-repeat: no-repeat;
  margin-right: 5px;
}
.list  {
  margin: 15px auto;
  border: 1px solid #007c70;
  background: #f6f6f6;
  width: 755px;
}
.intro_content {
  font-size: 16px;
